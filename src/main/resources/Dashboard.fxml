<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Dashboard">

    <!-- Button for "Pret" -->
    <Button fx:id="Pret" layoutX="33.0" layoutY="150.0" onAction="#Pret" prefHeight="100" prefWidth="104.0" style="-fx-font-size: 20;" text="Pret" />

    <!-- Button for "Demande" -->
    <Button fx:id="Demande" layoutX="168.0" layoutY="150.0" onAction="#Demande" prefHeight="100" prefWidth="118.0" style="-fx-font-size: 20;" text="Demande">
      <font>
         <Font size="2.0" />
      </font></Button>
    <Button fx:id="statistique" layoutX="312.0" layoutY="150.0" mnemonicParsing="false" onAction="#statistiqueon" prefHeight="100.0" prefWidth="118.0" style="-fx-font-size: 20;" text="statistique">
      <font>
         <Font size="15.0" />
      </font></Button>
   <Button fx:id="simulator" layoutX="448.0" layoutY="150.0" mnemonicParsing="false" onAction="#simulatoron" prefHeight="100.0" prefWidth="118.0" style="-fx-font-size: 20;" text="simulator">
      <font>
         <Font size="15.0" />
      </font>
   </Button>

</AnchorPane>
